{
  "env": { "browser": true },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "globals": {
    "__dirname": true
  },
  "ignorePatterns": ["dist/*"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "rules": {
      "no-unused-vars": "off"
  }
}
